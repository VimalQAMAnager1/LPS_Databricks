# TestNG suite for running Cucumber BDD tests
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd" >
<suite name="CucumberBDD" verbose="1" parallel="tests" thread-count="2">
    <test name="Cucumber BDD Tests">
        <classes>
            <class name="testrunner.RunCucumberTest"/>
        </classes>
    </test>
</suite>
